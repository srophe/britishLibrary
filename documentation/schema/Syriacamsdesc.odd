                    <!-- linking module -->
                    <moduleRef key="linking" include="seg"/>

                    <!-- msdescription module -->
                    <moduleRef key="msdescription"/>

                    <!-- namesdates module -->
                    <!-- Syriaca, namesdates: add tei:listPerson, tei:listPlace, tei:listRelation, tei:person, tei:place, tei:relation -->
                    <moduleRef key="namesdates" include="
                        addName 
                        country 
                        district 
                        forename 
                        geo geogFeat geogName
                        listRelation
                        persName person place placeName 
                        relation region 
                        settlement surname 
