                    <!-- dimensions/@type
            
            original: binding, book, folia, leaf, leaves, line-height, membrane, 'number of photograph', printed, ruled, ruledColumn, ruling, unknown, written
            
            In conversions:
            binding: keep 
            book: keep
            folia: keep 
            'number of folia', change to 'folia'
            leaf: keep 
            leaves: change to leaf
            line-height: keep 
            membrane: keep 
            photograph: other
            printed, ruled: keep 
            ruledColumn: change to 'ruled'
            ruling: change to 'ruling'
            unknown: change to 'other'
            written: keep
            
            Suggested values:
            binding, folia, leaf, line-height, ruled, written, other
            
            -->
                    <elementSpec ident="dimensions" module="msdescription" mode="change">
                        <classes mode="change">
                            <memberOf key="att.global" mode="delete"/>
                            <memberOf key="att.ranging" mode="delete"/>
                        </classes>
                        <content>
                            <sequence preserveOrder="true">
                                <elementRef key="height" minOccurs="0" maxOccurs="1"/>
                                <elementRef key="width" minOccurs="0" maxOccurs="1"/>
                            </sequence>
                        </content>
                        <constraintSpec ident="dimensions-unit.check" scheme="schematron">
                            <constraint>
                                <sch:rule context="//tei:dimensions">
                                    <sch:assert test="@unit" role="error"> The unit of measurement
                                        must be specified in the unit attribute on the dimensions
                                        element </sch:assert>
                                </sch:rule>
                            </constraint>
                        </constraintSpec>
                        <attList>
                            <attDef ident="type" mode="change">
                                <valList mode="replace" type="closed">
                                    <valItem ident="leaf">
                                        <desc>The dimensions concerns the leaf</desc>
                                    </valItem>

                                    <!-- Syraica: These values allowed in the consolidated BL schema but we aren't using them.
                                    <valItem ident="binding">
                                        <desc>The dimensions concerns the binding(s)</desc>
                                    </valItem>
                                    <valItem ident="folia">
                                        <desc>The dimensions concerns the folia as a whole</desc>
                                    </valItem>
                                    <valItem ident="line-height">
                                        <desc>The dimensions concerns the line-height</desc>
                                    </valItem>
                                    <valItem ident="ruled">
                                        <desc>The dimensions concerns the ruling</desc>
                                    </valItem>
                                    <valItem ident="written">
                                        <desc>The dimensions concerns the written area</desc>
                                    </valItem>
                                    <valItem ident="other">
                                        <desc>The dimensions concerns other/unspecified aspects (you
                                            may also provide a custom value)</desc>
                                    </valItem>-->
                                </valList>
                            </attDef>
                            <attDef ident="unit" mode="change" usage="req">
                                <valList type="closed">
                                    <valItem ident="inch"/>
                                </valList>
                            </attDef>
                        </attList>
                    </elementSpec>


