                <head>Schema</head>
                <schemaSpec ident="msdesc" start="TEI">
                    <!-- Modules -->
                    <!-- TEI module include for overall infrastructure -->
                    <moduleRef key="tei"/>

                    <!-- core module -->
                    <!-- Syriaca, core: add tei:ptr, tei:foreign-->
                    <moduleRef key="core" include="
                        abbr add address addrLine author analytic
                        bibl biblScope biblStruct
                        choice citedRange cit corr 
                        date del desc 
                        editor email expan 
                        foreign 
                        gap graphic 
                        head hi 
                        imprint item 
                        l label lb 
                        lg list listBibl 
                        measure monogr
                        name note num 
                        orig 
                        p pb postCode ptr publisher pubPlace 
                        q quote 
                        ref reg relatedItem resp respStmt 
                        series sic street 
                        term textLang title 
                        unclear 
                    "/>

                    <!-- figures module -->
                    <!-- Syriaca, figures: add tei:cell, tei:table and tei:row-->
                    <moduleRef key="figures" include="cell
                        figDesc figure formula 
                        row
                        table"/>

                    <!-- gaiji module -->
                    <!-- Syriaca, add gaiji module -->
                    <!-- Syriaca, gaiji: add tei:g, tei:glyph, tei:mapping -->
                    <moduleRef key="gaiji" include="charDecl
                        g glyph
                        mapping"/>

                    <!-- header module -->
                    <!-- confirm that we don't want tei:charDecl -->
                    <!-- Syriaca, header: add tei:editorialDecl, tei:interpretation, tei:language, and tei:langUsage-->
                    <moduleRef key="header" include="authority availability 
                        catDesc category change classDecl 
                        distributor 
                        edition editionStmt editorialDecl encodingDesc extent 
                        fileDesc funder 
                        handNote 
                        idno interpretation
                        keywords 
                        language langUsage licence 
                        principal profileDesc projectDesc publicationStmt 
                        revisionDesc 
                        scriptNote sourceDesc sponsor 
